<%= render :partial => 'tenant/leave_requests/leave_requests', :locals => { :leave_requests => @leave_requests } %>
$('#download_csv').attr('href', '<%= staff_leave_requests_path(
  :format => :csv,
  :leave_request_filter => {
    :status => params[:leave_request_filter][:status],
    :date_from => params[:leave_request_filter][:date_from],
    :date_to => params[:leave_request_filter][:date_to],
    :filter_by => params[:leave_request_filter][:filter_by],
    :location_id => params[:leave_request_filter][:location_id],
    :department_id => params[:leave_request_filter][:department_id],
    :employee_id => params[:leave_request_filter][:employee_id],
    :requires_documentation_only => params[:leave_request_filter][:requires_documentation_only]
  }
) %>');
