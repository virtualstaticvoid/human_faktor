employee_name,date_from,date_to,leave_type,duration,status
<% leave_requests.each do |leave_request| %>
"<%= leave_request.employee.full_name %>",<%= leave_request.date_from %>,<%= leave_request.date_to %>,"<%= leave_request.leave_type.to_s %>",<%= leave_request.duration.round(2) %>,"<%= leave_request.status_text %>"
<% end %>
